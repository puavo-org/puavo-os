msgstr "Indiquer le nom d'utilisateur par défaut à utiliser pour les requêtes."

# type: TP
#. type: TP
#: client.conf.5:97
#, no-wrap
msgid "B<UserAgentTokens None>"
msgstr "B<UserAgentTokens> I<None>"

# type: TP
#. type: TP
#: client.conf.5:99
#, no-wrap
