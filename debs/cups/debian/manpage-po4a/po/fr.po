"    cc `cups-config --cflags --ldflags` -o filter filter.c \\e\n"
"        `cups-config --libs`\n"
msgstr ""
"    cc `cups-config --cflags --ldflags` -o filter filter.c \\e\n"
"        `cups-config --libs`\n"

