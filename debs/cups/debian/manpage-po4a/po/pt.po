msgid "cupstestppd - test conformance of ppd files (deprecated)"
msgstr "cupstestppd - testa conformidade de ficheiros ppd (descontinuado)"

#. type: Plain text
#: cupstestppd.1:36
msgid ""
"B<cupstestppd> [ B<-I> I<category> ] [ B<-R> I<rootdir> ] [ B<-W> "
"I<category> ] [ B<-q> ] [ B<-r> ] [ B<-v>[B<v>] ] I<filename.ppd[.gz]> "
"[ ...  I<filename.ppd[.gz]> ]"
msgstr ""
"B<cupstestppd> [ B<-I> I<category> ] [ B<-R> I<rootdir> ] [ B<-W> "
"I<category> ] [ B<-q> ] [ B<-r> ] [ B<-v>[B<v>] ] I<filename.ppd[.gz]> "
"[ ...  I<filename.ppd[.gz]> ]"

#. type: Plain text
#: cupstestppd.1:52
