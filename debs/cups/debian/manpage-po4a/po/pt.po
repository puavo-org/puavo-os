msgstr "Reporta as opções de compilação necessárias."

#. type: TP
#: cups-config.1:69
#, no-wrap
msgid "B<--datadir>"
msgstr "B<--datadir>"

#. type: Plain text
#: cups-config.1:72
msgid "Reports the default CUPS data directory."
msgstr "Reporta o directório de dados do CUPS predefinido."

#. type: TP
#: cups-config.1:72 ippeveprinter.1:91 ippfind.1:194 ipptool.1:94
#, no-wrap
